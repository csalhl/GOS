page {
  background-color: #fff;
  font-size: 16px;
  font-family:  Arial, Helvetica, sans-serif;
  -webkit-overflow-scrolling : touch;
}
.page{
  position: relative;
}
.logo {
  width: 160rpx;
  height: 160rpx;
  display: block;
  margin: 150rpx auto 0px auto;
}

.logo-txt {
  text-align: center;
  display: inline-block;
  width: 100%;
  margin-top: 38rpx;
  font-size: 30rpx;
  color: #757980;letter-spacing:6rpx;
}

.btn-area {
  text-align: center;margin:0rpx 70rpx;

}

.btn-area button {
  overflow: visible;
  font-size: 33rpx;
  padding: 10rpx 0rpx;
  background: #245098!important;
  margin-bottom: 30rpx;

}

.fl {
  float: left;
}

.fr {
  float: right;
}

.color-red {
  color: #fa5151;
}

.footer {
  position: absolute;
  bottom: 30rpx;
  width: 85%;
  text-align: center;
  font-size: 26rpx;
  margin: auto;left: 0px;right: 0px;
}

.footer .txt {
  background: #fff;
  display: inline-block;
  padding: 0rpx 25rpx;
  position: relative;
  z-index: 3;
  color: #999999;
}

.footer .line{
  width: 100%;
  height: 2rpx;
  z-index: 1;
  background: #e1e3e5;
  position: absolute;
  top:18rpx;
  left: 0px;
  right: 0px;
  margin: auto;
}
.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height: 45px;
  background: #2C5CFF;
  z-index: 999;
}
.custom text{
  color: #fff;
  font-size: 34rpx;
  font-weight: 500;
  max-width: 280rpx;
}
.empty_custom{
  height: 45px;
  width: 100%;
}