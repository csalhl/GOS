.chose {
  padding:20rpx 50rpx;
  text-align: left;
  margin-top: 100rpx;
}

.chose text {
  font-size: 29rpx;
}

.chose button {
  background: #fff;
  line-height: 1;
  border-radius: 10rpx;
  padding: 30rpx 0rpx;
  margin: 30rpx auto;
  overflow: visible;
  font-size: 28rpx;
  text-align: left;font-weight: 600
}
.chose button .chose-img{
  width: 120rpx;
  height: 120rpx;border-radius: 50%;display: inline-block;vertical-align: middle;
  background: #f2f3f5;margin: 0rpx 40rpx;
}
.chose button image{
  width: 100%;height: 100%;border-radius: 50%;
}