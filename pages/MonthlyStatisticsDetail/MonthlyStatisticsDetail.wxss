/* pages/MonthlyStatistics/MonthlyStatistics.wxss */

page {
  background: #ebf2ff;
}

.page {
  position: absolute;
  width: 100%;
  height: 100%;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.statisticsDetail {
  text-align: left;
  position: relative;
}

.statisticsDetail .box {
  width: 580rpx;
  margin: auto;
}

.statisticsDetail .box .title {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  height: 94rpx;
}

.statisticsDetail .box .content {
  width: 537rpx;
  background: #fff;
  overflow: hidden;
  box-shadow: 0 6rpx 10rpx 2rpx #b1c9f999; /*for Android*/
  -webkit-box-shadow: 14px 14px 0px 0px #b1c9f999; /*for IOS*/
  margin: auto;
  padding: 180rpx 0rpx 317rpx 82rpx;
  box-sizing: border-box;
}

.statisticsDetail .box .content view {
  display: block;
  font-size: 32rpx;
  color: #494b4c;
  margin: 31rpx 0rpx;
}

.statisticsDetail .box .content view text {
  font-size: 36rpx;
  color: #31589c;
  margin: 0rpx 10rpx;
  font-weight: 600;
}

.bottom-bg {
  display: block;
  width: 496rpx;
  height: 431rpx;
  margin: -85rpx auto 0 auto;
}
