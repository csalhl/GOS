<view>
  <view class='nav bg-white' style='height:{{navH}}px'>
    <view class='nav-title'>
      <image src='../images/back.png' class='back' wx:if="{{isShowBack}}" bindtap='goback'></image>
      <view class='nav-search' wx:if="{{isShowSearch}}">
        <i-icon type="search" size='22' color='#b2b2b2' />
        <input type='text' placeholder='搜索工单' placeholder-style="color:#b2b2b2;"></input>
      </view>
      {{innerText}}
    </view>
  </view>
  <view style='height:{{navH}}px'></view>
</view>