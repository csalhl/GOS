/* pages/selectDevice/selectDevice.wxss */

page {
  background: #f0f2f5;
}

.parent-title {
  padding-left: 50rpx;
  background: url(data:image/png;base64,) no-repeat center left;
  background-size: 36rpx 36rpx;
}
.van-collapse-item__content {
  padding: 0 !important;
}
.van-collapse-item{margin-top: 24rpx}
.ul{
  overflow: hidden
}
.ul .li{
  display: block;
  padding: 25rpx 35rpx;
}
.ul .li text {
  font-size: 28rpx;
  color: #1e2021;
}
.ul .li .check {
  display: inline-block;
  float: right;
  box-sizing: border-box;
  width: 40rpx;
  height: 40rpx;
  color: transparent;
  font-size: 32rpx;
  border-radius: 50%;
  line-height: inherit;
  text-align: center;
  border: 1rpx solid #e5e5e5;
  -webkit-transition: 0.2s;
  transition: 0.2s;
  vertical-align: middle
}
.ul .li .checked{
  border-color: #5cb234;background: #5cb234;
}
.getOrder {
  color: #fff;
  width: 100%;
  position: fixed;
  bottom: 0px;
  left: 0px;
  display: flex;
}

.getOrder-btn {
  background: #245098;
  font-size: 30rpx;
  text-align: center;
  padding: 30rpx 0rpx;
  flex: auto;
}

.getOrder .notes {
  width: 50%;
  background: #fff;
  text-align: center;
  color: #1e2021;
  padding: 30rpx 0rpx;
}
